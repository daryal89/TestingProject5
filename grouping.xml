<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="TestNG Suite">
	<groups>
		<run>
			<include name="sanity" />
			<include name="smoke" />
			<include name="regression" />
		</run>
	</groups>
	<test name="Test">
		<classes>
			<class name="testng.DependsOnMethodsTestNG" />
			<class name="testng.DataProviderTestNG" />
			<class name="com.qa.TestProjecgt.TestBase" />
			<class name="com.qa.TestProjecgt.Day1" />
			<class name="com.qa.TestProjecgt.Day2" />
			<class name="testng.JavaMethodTest" />
			<class name="testng.JavaTest" />
			<class name="testng.AssertionsTest" />
		</classes>
	</test>
</suite>
